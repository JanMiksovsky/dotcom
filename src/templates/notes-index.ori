(notes) => page.ori({
  title: "Notes"
  @text: `
    <h1>${ title }</h1>
    <ol reversed class="list-unstyled posts-list">

    ${ map(notes, (note) => `
      <li>
      <article class="note-single outline">
        <small class="timestamp"><a href="${ note/path }">${ date.ori(note/date) }</a></small>
        ${ note/@text }
      </article>  
      </li>
    `) }

    </ol>
  `
})